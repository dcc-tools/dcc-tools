pkgname=dcc-tools
_pkgname=dcc-tools
pkgver=1.0.0
pkgrel=1
url='https://github.com/tvillega/${_pkgname}/tree/master'
source=("${pkgname}-${pkgver}::git+https://github.com/tvillega/${_pkgname}.git")

build() {
  :
}

package() {
  cd "${srcdir}/${pkgname}-${pkgver}"
  install -Dm744 imprime  "${pkgdir}/${pkgname}/bin/imprime"
  install -Dm744 sitioweb "${pkgdir}/${pkgname}bin/sitioweb"
}
